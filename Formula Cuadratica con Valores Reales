#include <iostream>
#include <math.h>
using namespace std;
int main()
{   
    float a,b,c,x1,x2,d;        
    cout<<"Ingrese el valor de x^2: ";    
    cin>>a;        
    cout<<"Ingrese el valor de x: ";    
    cin>>b;        
    cout<<"Ingrese el termino independiente: ";    
    cin>>c;        
    d=b*b-(4*a*c);        
    x1=(-b+sqrt(d))/(2*a);   
    x2=(-b-sqrt(d))/(2*a);      
    cout<<"El primer valor de x es: "<<x1<<endl;    
    cout<<"El segundo valor de x es: "<<x2;     
    return 0;
}
